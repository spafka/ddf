<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <!-- 控制台打印日志 -->
        <Console name="consolePrint" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>


    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="consolePrint"/>
        </Root>
    </Loggers>
</Configuration>
